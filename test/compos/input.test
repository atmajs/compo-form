UTest({
	$config: {
		'http.process': {
			command: 'atma custom examples/index',
			matchReady: '/Listen /'
		}
	},
	$before (next) {
		UTest
			.server
			.request('http://localhost:5771/examples/index.html?input')
			.done((doc, win) => {
				next(doc, win);
			});
	},
	'input template' (done, doc, win) {
		UTest
			.domtest(doc.body, `
				find('.form-horizontal') {
					has ('.col-sm-3');
					has ('.col-sm-9');
					find ('input.foo_someString') {
						val Foo;
						do type Baz;
						
						val FooBaz;
					}
				}
			`)
			.always(() => done(doc, win));
	},
	
})