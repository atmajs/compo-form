{
  "name": "compo-form",
  "version": "0.10.23",
  "devDependencies": {
    "atma": ">0.9.26",
    "atma-loader-babel": "latest",
    "atma-server": "^0.1.22",
    "babel-preset-es2015": "^6.18.0"
  },
  "scripts": {
    "test": "atma test",
    "examples": "atma custom examples/index",
    "build": "atma"
  },
  "atma": {
    "plugins": [
      "atma-loader-babel"
    ],
    "settings": {
      "atma-loader-babel": {
        "extensions": [
          "es6",
          "test"
        ],
        "babel": {
          "presets": [
            "es2015"
          ]
        }
      }
    }
  },
  "dependencies": {
    "maskjs": "*"
  }
}
