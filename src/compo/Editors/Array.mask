let Array {
	
	function getArray () {
		return mask.obj.get(this.model, this.attr.property);
	}
	
	slot arrayItemRemove (event) {
		var model = $(event.target).model(),
			arr = this.getArray(),
			i = arr.indexOf(model);
		arr.splice(i, 1);
	}
	
	slot arrayItemAdd () {
		var arr = this.getArray(),
			item = {};
		arr.push(item);
	}
	
	
	
	include ItemLayout {
		@element {
			@header;
			ul.list-unstyled > +each (@attr.property) {
				li style='position:relative; padding: 0px 15px; box-sizing: border-box;' {
					@template;
				}
			}
			@footer;
		}
	}
	
	
}
